{
  "name": "kf2queryapp",
  "version": "1.0.0",
  "description": "Query Killing Floor 2 server",
  "author": {
    "name": "FishMissile",
    "email": "FishMissile@gmail.com",
    "url": "http://jaggedcommandos.net"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd client && yarn build && cd .. && yarn start",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "engines": {
    "node": "8.11.2",
    "npm": "6.2.0"
  },
  "dependencies": {
    "apollo-client": "^2.3.7",
    "cors": "^2.8.4",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "gamedig": "^1.0.46",
    "graphql": "^0.13.2",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "concurrently": "^3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FishMissile/kf2queryapp2.git"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ]
}
